<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="/_includes/assets/fa/css/all.min.css" rel="stylesheet" media="print" onload="this.media='screen'">
  <link rel="preconnect" href="//fonts.googleapis.com/" crossorigin>
  <title>{%- if title !== 'Welcome' -%}{{ title }} | Amy Dawson{%- else -%}Amy Dawson{%- endif -%}</title>
  {% set meta_description %}
    {%- if description -%}{{ description }}{%- else -%}Amy Dawson is a middle school math teacher, professional baker, and athlete based in upstate New York.{%- endif -%}
  {% endset %}
  <meta name="description" content="{{ meta_description }}"/>
  <link rel="alternate" href="/feed.xml" title="Amy Dawson blog feed, by Amy Dawson" type="application/atom+xml">
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@skirt_runner" />
  <meta name="twitter:creator" content="@skirt_runner" />
  <meta name="twitter:description" content="{{ meta_description }}"/>
  <meta property="og:url" content="https://skirtrunner.com/{%- if permalink !== '/' -%}{{ permalink }}{%- endif -%}" />
  <meta property="og:title" content="{%- if title !== 'Welcome' -%}{{ title }} via Amy Dawson{%- else -%}Amy Dawson's Web Site{%- endif -%}" />
  <meta property="og:description" content="{{ meta_description }}"/>
  {%- if hero -%}
  <meta property="og:image" content='https://skirtrunner.com{{ hero }}?nf_resize=smartcrop&w=1200&h=630' />
  <meta property="og:image:width" content='1200' />
  <meta property="og:image:height" content='630' />
  {%- endif -%}
  <link rel="icon" type="image/png" sizes="512x512"  href="/static/img/android-chrome.png">
  <link rel="icon" type="image/png" sizes="192x192"  href="/static/img/android-chrome.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/static/img/favicon-16x16.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/static/img/favicon-32x32.png">
  <link rel="apple-touch-icon" sizes="180x180" href="/static/img/apple-icon.png">
  <script defer src="/_includes/assets/fa/js/all.js"></script>
  <script defer src="/_includes/assets/js/search.js"></script>
  <script defer src="/_includes/assets/js/flickity.pkgd.min.js"></script>
  <script defer src="/_includes/assets/js/fullscreen.js"></script>
  {% set css %}
    {% include "assets/css/inline.css" %}
    {% include "assets/css/flickity.min.css" %}
    {% include "assets/css/fullscreen.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-3965818-14"></script>
  <script>
  if (document.location.hostname.search("skirtrunner.com") !== -1) {
  window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'UA-3965818-14');
  }
  </script>
</head>
